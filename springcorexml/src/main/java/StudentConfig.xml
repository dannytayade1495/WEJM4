<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.beans.StudentBean">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name">
			<value>Abhishek</value>
		</property>
		<property name="email">
			<value>abhishek@gmail.com</value>
		</property>
	</bean>
	
	<bean class="com.jspiders.springcorexml.beans.StudentBean"
	name="student1">
		<property name="id" value="2"/>
		<property name="name" value="Bhavika"/>
		<property name="email" value="bhavika@gmail.com"/>
	</bean>
	
	<bean class="com.jspiders.springcorexml.beans.StudentBean"
	name="student2">
		<constructor-arg name="id" value="3"/>
		<constructor-arg name="name" value="Chitra"/>
		<constructor-arg name="email" value="chitra@gmail.com"/>
	</bean>

</beans>